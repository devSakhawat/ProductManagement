               <h4 class="card-title">Toner Usage Calculation Page</h4><hr />
            </div>
            <div class="card-body">
               <div class="table-responsive" id="TonerUsageForm">
                  <table class="table bg-light-secondary">
                     <thead>
                        <tr>
                           <th width="25%">
                              <select class="form-control" id="CustomerId" onchange="getProject(event)">
                                 <option value="">Select Customer</option>
                              </select>
                           </th>
                           <th width="25%">
                              <select class="form-control" id="ProjectId" onchange="getMachine(event)">
                                 <option value="">Select Project</option>
                              </select>
                           </th>
                           <th width="25%">
                              <select class="form-control" id="MachineId" onchange="getToner(event)">
                                 <option value="">Select Machine</option>
                              </select>
                           </th>
                           <th width="25%"><button type="button" onclick="CalculateValues()" class="btn btn-primary waves-effect waves-float waves-light">Add One Info</button></th>
                        </tr>
                     </thead>
                     <thead id="TonerPercentage">

                     </thead>
                  </table>

                  <table class="table bg-light-secondary">
                     <thead>
                        <tr>
                           <th width="33.33%">
                              <input type="text" class="form-control prevCounter" placeholder="Previous Counter" autocomplete="off" id="PrevCounter" />
                           </th>
                           <th width="33.33%">
                              <input type="text" class="form-control curtCounter" placeholder="Current Counter" autocomplete="off" id="CurtCounter" />
                           </th>
                           <th width="33.33%">
                              <input type="text" class="form-control totalCounter" placeholder="Total Counter" autocomplete="off" id="TotalCounter" />
                           </th>
                        </tr>
                     </thead>
                  </table>

                  <table class="table table-bordered mt-1" id="AddRemoveItem">
                     <tr>
                        <th class="wd-15p">Machine SN</th>
                        <th class="wd-15p">InMachine Toner</th>
                        <th class="wd-15p">Monthly Counter</th>
                        <th class="wd-15p">Monthly Counter</th>
                        <th class="wd-15p">Item Total</th>
                        <th class="wd-15p">Item Profite</th>
                        <th class="wd-10p td2 wd-100p"></th>
                     </tr>
                     <tfoot>
                        <tr>
                           <td class="wd-15p PrevBWToner" id="prevBWToner"><input type="text" class="form-control" placeholder="Previous BW Toner" /></td>
                           <td class="wd-15p PrevColourToner" id="prevColourToner">Previous Colour Toner</td>
                           <td class="wd-15p DeliBWToner" id="deliBWToner">Delivery BW Toner</td>
                           <td class="wd-15p DeliColourToner" id="deliColourToner">Delivery Colour Toner</td>
                           <td class="wd-15p InHouseTotalToner" id="inHouseTotalToner">InhouseToner</td>
                           <td class="wd-15p MachineTotalToner" id="machineTotalToner">Machine Toner</td>
                           <td class="wd-10p td2 wd-100p"></td>
                        </tr>
                     </tfoot>
                  </table>


